{
    "breadcrumbs.enabled": true,
    "cSpell.language": "en,fr",
    "editor.fontFamily": "Menlo, Consolas, 'Courier New', monospace",
    "editor.formatOnPaste": true,
    "editor.minimap.enabled": false,
    "editor.multiCursorModifier": "ctrlCmd",
    "editor.renderWhitespace": "all",
    "emmet.showSuggestionsAsSnippets": true,
    "emmet.triggerExpansionOnTab": true,
    "explorer.autoReveal": false,
    "files.autoSave": "onFocusChange",
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true,
    "files.trimTrailingWhitespace": true,
    "git.autofetch": true,
    "git.autoStash": true,
    "html.format.endWithNewline": true,
    "namespaceResolver.highlightOnOpen": true,
    "namespaceResolver.highlightOnSave": true,
    "namespaceResolver.showMessageOnStatusBar": true,
    "namespaceResolver.sortAlphabetically": true,
    "php-docblocker.author": "Christian Genc",
    "window.zoomLevel": 0,
}
